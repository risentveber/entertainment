<div class = 'card-sp first-left-block'>
  <div class = 'block-title'>
    <%= link_to 'Экипаж', community_members_path(@community) %>
  </div>
  <div class = 'block-objects'>
    <% @community.users.limit(5).each do |u| %>
      <div class = 'preview-object'>
          <div class = "preview-object-avatar-mini" style="background: url(
          <%= u.avatar.thumb %>
          ) no-repeat; background-size: cover;">
          </div>
          <div class="preview-object-info-mini">
            <div class = 'object-text'>
              <%= link_to u.name, user_path(u) %>
             </div>
          </div>
      </div>
      <div class = 'clearboth'>
      </div>
    <% end %>
  </div>
</div>
